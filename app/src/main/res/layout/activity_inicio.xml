<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    tools:context=".activity_inicio">

    <Button
        android:id="@+id/bt_link"
        android:layout_width="127dp"
        android:layout_height="114dp"
        android:layout_marginStart="272dp"
        android:layout_marginTop="360dp"
        android:layout_marginEnd="3dp"
        android:background="@android:color/transparent"
        android:backgroundTint="@color/coloriconos"
        android:onClick="cobro"
        app:icon="@drawable/_41"
        app:iconTint="@color/coloriconos"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bt_link4"
        android:layout_width="127dp"
        android:layout_height="114dp"
        android:layout_marginStart="272dp"
        android:layout_marginTop="454dp"
        android:layout_marginEnd="3dp"
        android:background="@android:color/transparent"
        android:backgroundTint="@color/coloriconos"
        android:onClick="cobro"
        app:icon="@drawable/_46"
        app:iconTint="@color/coloriconos"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.024"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bt_link1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="259dp"
        android:layout_marginTop="90dp"
        android:background="@android:color/transparent"
        android:backgroundTint="@color/coloriconos"
        android:onClick="cobro"
        app:icon="@drawable/_43"
        app:iconTint="@color/coloriconos"
        app:layout_constraintBottom_toTopOf="@+id/bt_link3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bt_link2"
        android:layout_width="116dp"
        android:layout_height="114dp"
        android:layout_marginStart="263dp"
        android:layout_marginTop="260dp"
        android:layout_marginEnd="35dp"
        android:background="@android:color/transparent"
        android:backgroundTint="@color/coloriconos"
        android:onClick="denuncias"
        app:icon="@drawable/_45"
        app:iconTint="@color/coloriconos"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.002"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bt_link3"
        android:layout_width="116dp"
        android:layout_height="114dp"
        android:layout_marginStart="259dp"
        android:layout_marginTop="190dp"
        android:layout_marginBottom="1dp"
        android:background="@android:color/transparent"
        android:backgroundTint="@color/coloriconos"
        android:onClick="mapa"
        app:icon="@drawable/_40"
        app:iconTint="@color/coloriconos"
        app:layout_constraintBottom_toTopOf="@+id/bt_link2"
        app:layout_constraintEnd_toStartOf="@+id/bt_link1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.51" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/_28" />

</androidx.constraintlayout.widget.ConstraintLayout>